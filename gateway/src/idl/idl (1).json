{
  "version": "0.1.0",
  "name": "tokengate",
  "instructions": [
    {
      "name": "registerApi",
      "accounts": [
        {"name": "signer", "isMut": true, "isSigner": true},
        {"name": "registry", "isMut": true, "isSigner": false},
        {"name": "systemProgram", "isMut": false, "isSigner": false}
      ],
      "args": [
        {"name": "name", "type": "string"},
        {"name": "backendUrl", "type": "string"},
        {"name": "rateLimit", "type": "u32"},
        {"name": "pricePerCall", "type": "u64"}
      ]
    },
    {
      "name": "purchaseAccess",
      "accounts": [
        {"name": "buyer", "isMut": true, "isSigner": true},
        {"name": "registry", "isMut": true, "isSigner": false},
        {"name": "accessKey", "isMut": true, "isSigner": false},
        {"name": "ownerWallet", "isMut": true, "isSigner": false},
        {"name": "systemProgram", "isMut": false, "isSigner": false}
      ],
      "args": [{"name": "apiId", "type": "publicKey"}]
    },
    {
      "name": "logUsage",
      "accounts": [
        {"name": "registry", "isMut": false, "isSigner": false},
        {"name": "accessKey", "isMut": true, "isSigner": false},
        {"name": "usageLog", "isMut": true, "isSigner": false},
        {"name": "accessKeyOwner", "isMut": true, "isSigner": true},
        {"name": "systemProgram", "isMut": false, "isSigner": false}
      ],
      "args": [{"name": "calls", "type": "u32"}]
    },
    {
      "name": "withdrawEarnings",
      "accounts": [
        {"name": "owner", "isMut": true, "isSigner": true},
        {"name": "registry", "isMut": true, "isSigner": false},
        {"name": "ownerWallet", "isMut": true, "isSigner": false},
        {"name": "systemProgram", "isMut": false, "isSigner": false}
      ],
      "args": [{"name": "amount", "type": "u64"}]
    },
    {
      "name": "revokeAccess",
      "accounts": [
        {"name": "owner", "isMut": false, "isSigner": true},
        {"name": "registry", "isMut": false, "isSigner": false},
        {"name": "accessKey", "isMut": true, "isSigner": false}
      ],
      "args": []
    },
    {
      "name": "pauseApi",
      "accounts": [
        {"name": "owner", "isMut": false, "isSigner": true},
        {"name": "registry", "isMut": true, "isSigner": false}
      ],
      "args": []
    },
    {
      "name": "unpauseApi",
      "accounts": [
        {"name": "owner", "isMut": false, "isSigner": true},
        {"name": "registry", "isMut": true, "isSigner": false}
      ],
      "args": []
    }
  ],
  "accounts": [
    {
      "name": "AccessKey",
      "type": {
        "kind": "struct",
        "fields": [
          {"name": "apiId", "type": "publicKey"},
          {"name": "owner", "type": "publicKey"},
          {"name": "callsRemaining", "type": "u32"},
          {"name": "active", "type": "bool"},
          {"name": "bump", "type": "u8"}
        ]
      }
    },
    {
      "name": "ApiRegistry",
      "type": {
        "kind": "struct",
        "fields": [
          {"name": "owner", "type": "publicKey"},
          {"name": "name", "type": "string"},
          {"name": "backendUrl", "type": "string"},
          {"name": "rateLimit", "type": "u32"},
          {"name": "pricePerCall", "type": "u64"},
          {"name": "totalCalls", "type": "u64"},
          {"name": "totalEarnings", "type": "u64"},
          {"name": "paused", "type": "bool"},
          {"name": "bump", "type": "u8"}
        ]
      }
    },
    {
      "name": "UsageLog",
      "type": {
        "kind": "struct",
        "fields": [
          {"name": "apiId", "type": "publicKey"},
          {"name": "user", "type": "publicKey"},
          {"name": "totalCalls", "type": "u64"},
          {"name": "totalCost", "type": "u64"},
          {"name": "bump", "type": "u8"}
        ]
      }
    }
  ],
  "errors": [
    {"code": 6000, "name": "InvalidName", "msg": "Invalid name length"},
    {"code": 6001, "name": "InvalidRateLimit", "msg": "Invalid rate limit"},
    {"code": 6002, "name": "ApiPaused", "msg": "API is paused"},
    {"code": 6003, "name": "KeyInactive", "msg": "Access key is inactive"},
    {"code": 6004, "name": "NotAuthorized", "msg": "Not authorized"},
    {"code": 6005, "name": "InsufficientBalance", "msg": "Insufficient balance"},
    {"code": 6006, "name": "InvalidAmount", "msg": "Invalid amount"},
    {"code": 6007, "name": "AlreadyRevoked", "msg": "Already revoked"},
    {"code": 6008, "name": "AlreadyPaused", "msg": "Already paused"},
    {"code": 6009, "name": "NotPaused", "msg": "Not paused"},
    {"code": 6010, "name": "Overflow", "msg": "Overflow"}
  ]
}